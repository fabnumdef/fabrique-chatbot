{
  "name": "fabrique-chatbot-back",
  "version": "1.17.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy": "cp .env dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/cli": "^9.1.8",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "ansible-playbook-cli-js": "^2.0.5",
    "bcrypt": "^5.1.0",
    "camelcase-keys": "^7.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^6.7.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.2.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^2.0.0",
    "moment": "^2.29.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.0.7",
    "rxjs": "^7.8.0",
    "snakecase-keys": "^5.4.4",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.15",
    "@types/jest": "29.2.5",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@dto/(.*)$": "<rootDir>/core/dto/$1",
      "^@entity/(.*)$": "<rootDir>/core/entities/$1",
      "^@model/(.*)$": "<rootDir>/core/models/$1",
      "^@enum/(.*)$": "<rootDir>/core/enums/$1",
      "^@service/(.*)$": "<rootDir>/core/services/$1",
      "^@guard/(.*)$": "<rootDir>/core/guards/$1",
      "^@interceptor/(.*)$": "<rootDir>/core/interceptors/$1",
      "^@mock/(.*)$": "<rootDir>/../test/data/$1",
      "^@decorator/(.*)$": "<rootDir>/core/decorators/$1"
    }
  }
}
