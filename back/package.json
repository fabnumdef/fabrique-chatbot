{
  "name": "fabrique-chatbot-back",
  "version": "1.4.5",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy": "cp .env dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/bull": "^0.3.0",
    "@nestjs/cli": "^7.5.3",
    "@nestjs/common": "^7.5.4",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.4",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.3",
    "@nestjs/platform-express": "^7.5.4",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "ansible-playbook-cli-js": "^2.0.5",
    "bcrypt": "5.0.0",
    "bull": "^3.19.0",
    "camelcase-keys": "^6.2.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.2.3",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.2.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.6.3",
    "snakecase-keys": "^3.2.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.2.29",
    "xlsx": "^0.17.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.5.4",
    "@types/bull": "^3.14.4",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@dto/(.*)$": "<rootDir>/core/dto/$1",
      "^@entity/(.*)$": "<rootDir>/core/entities/$1",
      "^@model/(.*)$": "<rootDir>/core/models/$1",
      "^@enum/(.*)$": "<rootDir>/core/enums/$1",
      "^@service/(.*)$": "<rootDir>/core/services/$1",
      "^@guard/(.*)$": "<rootDir>/core/guards/$1",
      "^@interceptor/(.*)$": "<rootDir>/core/interceptors/$1",
      "^@mock/(.*)$": "<rootDir>/../test/data/$1",
      "^@decorator/(.*)$": "<rootDir>/core/decorators/$1"
    }
  }
}
