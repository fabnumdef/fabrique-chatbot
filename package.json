{
  "name": "fabrique-chatbot-back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy": "cp .env dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/bull": "^0.1.2",
    "@nestjs/common": "^7.4.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.1",
    "ansible-playbook-cli-js": "^2.0.5",
    "bcrypt": "^5.0.0",
    "bull": "^3.18.0",
    "camelcase-keys": "^6.2.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.0-rc.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.1",
    "handlebars": "^4.7.6",
    "helmet": "^3.22.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.19",
    "nestjs-redis": "^1.2.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "snakecase-keys": "^3.1.2",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.24",
    "xlsx": "^0.15.5"
  },
  "devDependencies": {
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.11.11",
    "@types/bull": "^3.14.2",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^25.2.4",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@dto/(.*)$": "<rootDir>/core/dto/$1",
      "^@entity/(.*)$": "<rootDir>/core/entities/$1",
      "^@model/(.*)$": "<rootDir>/core/models/$1",
      "^@enum/(.*)$": "<rootDir>/core/enums/$1",
      "^@service/(.*)$": "<rootDir>/core/services/$1",
      "^@guard/(.*)$": "<rootDir>/core/guards/$1",
      "^@interceptor/(.*)$": "<rootDir>/core/interceptors/$1",
      "^@mock/(.*)$": "<rootDir>/../test/data/$1",
      "^@decorator/(.*)$": "<rootDir>/core/decorators/$1"
    }
  }
}
